{% extends "base.html.twig" %}

{% block stylesheets %}
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="{{ asset('vendor/css/uikit.min.css') }}">
    <link rel="stylesheet" href="{{ asset('css/base/elements.css') }}">
    <link rel="stylesheet" href="{{ asset('css/base/typography.css') }}">
    <link rel="stylesheet" href="{{ asset('css/base/form.css') }}">
    <link rel="stylesheet" href="{{ asset('css/components/header.css') }}">
    <link rel="stylesheet" href="{{ asset('css/components/page.css') }}">
    <link rel="stylesheet" href="{{ asset('css/components/twitter.css') }}">
    <link rel="stylesheet" href="{{ asset('css/components/user.css') }}">
    <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico') }}" />
{% endblock %}

{% block body %}
    {{ include('system/messages.html.twig', { 'messages': app.flashes(['warning', 'danger', 'primary', 'success'])}) }}
    <div class="page-wrap uk-height-1-1">
        <div class="page uk-flex uk-flex-column uk-height-1-1">
            {% block page %}{% endblock %}
        </div>
    </div>
{% endblock %}

{% block javascripts %}
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <script src="{{ asset('vendor/js/jquery.min.js') }}"></script>
    <script src="{{ asset('vendor/js/jquery-ui-core.min.js') }}"></script>
    <script src="{{ asset('vendor/js/uikit.min.js') }}"></script>
    <script src="{{ asset('vendor/js/uikit-icons.min.js') }}"></script>
    {#FILEUPLOAD#}
    <script src="{{ asset('vendor/js/fileupload/jquery.iframe-transport.js') }}"></script>
    <script src="{{ asset('vendor/js/fileupload/jquery.fileupload.js') }}"></script>
    <script src="{{ asset('vendor/js/fileupload/jquery.fileupload-process.js') }}"></script>
    {#END FILEUPLOAD#}
    <script src="{{ asset('scripts/svistyn_add.js') }}"></script>
{% endblock %}